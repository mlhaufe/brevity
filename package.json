{
  "name": "brevity",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "npm run clean && npm run lint",
    "build": "webpack",
    "prebuild-nofix": "npm run clean && npm run lint-nofix",
    "build-nofix": "webpack",
    "clean": "rimraf ./dist && rimraf ./coverage",
    "clean-full": "rimraf ./dist && rimraf ./coverage && rimraf ./.cache && rimraf ./node_modules",
    "-debug": "node --debug-brk --inspect ./node_modules/jest/bin/jest -i",
    "prelint": "npm run type-check",
    "lint": "eslint src/ --ext .ts --fix",
    "prelint-nofix": "npm run type-check",
    "lint-nofix": "eslint src/ --ext .ts",
    "test": "jest --coverage",
    "type-check": "tsc -p tsconfig.json --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlhaufe/brevity.git"
  },
  "author": "Michael L Haufe <tno@thenewobjective.com> (https://thenewobjective.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mlhaufe/brevity/issues"
  },
  "homepage": "https://github.com/mlhaufe/brevity#readme",
  "devDependencies": {
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "eslint": "^7.3.1",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.1.0",
    "jest-junit": "^11.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.1",
    "ts-loader": "^7.0.5",
    "typescript": "^4.0.0-beta",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "files": [
    "dist"
  ]
}
