{
    "name": "@mlhaufe/brevity",
    "version": "0.13.0",
    "description": "A library for Feature-Oriented Programming",
    "keywords": [
        "Object Algebra",
        "Feature Oriented Programming",
        "Expression Problem",
        "Traits",
        "Algebraic Data Types",
        "Pattern Matching",
        "memoization",
        "fixed point",
        "memofix",
        "Family Polymorphism"
    ],
    "exports": {
        ".": {
            "types": "./index.d.ts",
            "import": "./index.mjs"
        }
    },
    "type": "module",
    "scripts": {
        "build": "webpack && npm install --prefix dist --package-lock-only && npm run emit-types",
        "emit-types": "tsc -p jsconfig.declaration.json",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mlhaufe/brevity.git"
    },
    "author": "Michael L Haufe <tno@thenewobjective.com> (https://thenewobjective.com)",
    "license": "AGPL-3.0-only",
    "bugs": {
        "url": "https://github.com/mlhaufe/brevity/issues"
    },
    "homepage": "https://github.com/mlhaufe/brevity#readme",
    "files": [],
    "devDependencies": {
        "@types/jest": "^29.2.3",
        "copy-webpack-plugin": "^11.0.0",
        "jest": "^29.3.1",
        "typescript": "^5.0.4",
        "webpack": "^5.83.1",
        "webpack-cli": "^5.0.1"
    }
}